#include<bits/stdc++.h>
using namespace std;
void binarySearch(int data_array[], int element, int len)
{
    int low = 0;
    int high = len;
    while (low <= high)
    {
        int mid = (low+high)/2;
        int guess = data_array[mid];
        
        if (guess == element)
        {
            cout<<"Element found at "<<mid<<"th index"<<endl;
            break;
        }
        
        else if (guess > element)
        {
            high = mid - 1;
        }
        
        else if (guess < element)
        {
            low = mid + 1;
        }
    }
    cout<<"Element not found";
}

int main()
{
    int n,element;
    cin>>n;
    int data_array[n];
    for(int i=0;i<n;i++){
        cin>>data_array[i];
    }
    cin>>element;
    binarySearch(data_array, element, n) ; 
    return 0;
}
